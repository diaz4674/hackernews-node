datasource db {
    provider = "postgresql"
    url      = "postgresql://postgres:number1@localhost:5432/mydb?schema=public"
}

generator client {
    provider = "prisma-client-js"
}

model Link {
    id          Int      @id @default(autoincrement())
    createdAt   DateTime @default(now())
    description String
    url         String
}
